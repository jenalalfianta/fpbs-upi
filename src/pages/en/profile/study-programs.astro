---
import Layout from '@/layouts/Layout.astro';
import ProgramStudiFpbs from '@/components/en/prodi/ProgramStudiFpbs.astro';
const slugId = 'page-prodi';
const pageHeading = 'Study Programs';
const pageSubtitle = 'Faculty of Language and Literature Education - UPI';
---

<Layout
  pageHeading={pageHeading}
  pageSubtitle={pageSubtitle}
  breadcrumbItems={[
    { label: 'Home', href: '/en/index.html' },
    { label: 'Study Programs' },
  ]}
  breadcrumbBackground="bg-[#f6f8ff] dark:bg-[#0b132a]"
  pageHeadingOverlay="linear-gradient(90deg, rgba(31,15,61,0.85) 0%, rgba(49,26,92,0.70) 55%, rgba(31,15,61,0.45) 100%)"
  pageHeadingAlign="left"
>
  <ProgramStudiFpbs/>
</Layout>
